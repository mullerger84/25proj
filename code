#스택 정의
class Stack :
    def __init__(self) :
        self.items = [1,1,1,1,1,1]
    def push(self,items) :
        self.items.append(items)
    def pop(self) :
        return self.items.pop()
    def isEmpty(self) :
        if len (self.items) == 0:
            return True
        else :
            return False
    def size(self) :
        return len(self.items)       
s= Stack()


print('파스코의 대여,반납,현황확인 중 원하는 것을 입력해주세요') #대여, 반납, 현황확인 중 선택
a = input()
def check(a) :
    for i in a :
        if i in '대여' :  #대여
            print('대여 할 사람의 이름 적어주세요.')  
            b = input()
            if not s.isEmpty() :
                s.pop()
            print('현재 남은 물품 수는', s.size(), ' 개 입니다.')
            break
        elif i in '반납' :  #반납
            s.push(1)
            print('반납할 사람의 이름을 적어주세요')
            c = input()
            print('현재 남은 물품 수는',s.size(),' 개 입니다.')
            break
        elif i in '현황확인' :  #현황확인
            print('현재 남은 물품 수는',s.size(), '개 입니다.')
            break
    if s.isEmpty() :
        return True
    else :
        return False
    return
if check(a) :
    print('남은 물품의 수가 없습니다.')
else :
    print('작업이 완료되었습니다!')
